        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-sphinx": "^2.0",
        "mdmsoft/yii2-admin": "2.7",
        "miloschuman/yii2-highcharts-widget": "*",
        "kartik-v/yii2-widget-depdrop": "@dev",
        "himiklab/yii2-recaptcha-widget": "*",
