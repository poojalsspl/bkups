        "mpdf/mpdf":"*",
        "2amigos/yii2-tinymce-widget" : "~1.1",
        "kartik-v/yii2-widget-spinner": "@dev",
        "loveorigami/yii2-jquery-loading": "@dev",
        "liyunfang/yii2-contextmenu": "^1.0",
